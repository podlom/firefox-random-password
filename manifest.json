{
  "manifest_version": 2,
  "name": "Random Passwords Generator",
  "version": "1.0.6",
  "description": "Generate secure random passwords.",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": ["storage"],
  "background": {
    "scripts": ["lib/browser-polyfill.js", "background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  }
}